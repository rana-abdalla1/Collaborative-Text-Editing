{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "max-len": [
            "error",
            120
        ],
        "filenames": "off",
        "no-invalid-this": "off"
    }
}